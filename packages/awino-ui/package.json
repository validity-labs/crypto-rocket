{
  "name": "@awino/ui",
  "version": "0.0.0",
  "description": "Awino web application based on Next.js",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "node ./scripts/env-check.js && next start",
    "type-check": "tsc",
    "test": "jest",
    "test:unit": "TZ=UTC jest",
    "test:unit:update-snap": "TZ=UTC jest --updateSnapshot",
    "test:pre-commit": "TZ=UTC jest --bail --findRelatedTests",
    "test:unit:watch": "TZ=UTC jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cy:prepare": "NODE_ENV=test npm run build",
    "cy:next": "tsc --project testing/cypress/next/tsconfig.server.json && NODE_ENV=test node testing/cypress/.cypress/server.js",
    "cy:open": "cypress open",
    "cy:open:full": "npm run cy:prepare && start-server-and-test cy:next 3000 cy:open",
    "cy:run": "cypress run ",
    "cy:run:full": "npm run cy:prepare && start-server-and-test cy:next 3000 cy:run",
    "cy:run:full-update": "npm run cy:prepare && start-server-and-test cy:next 3000 cy:run:update",
    "lint:check": "next lint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check 'src/**/*.{ts(x)?,json,css}'",
    "format:fix": "prettier src/**/{*.ts,*.tsx} --write"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.13.1",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@ethersproject/providers": "^5.5.3",
    "@ethersproject/units": "^5.5.0",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@mui/x-data-grid": "^5.6.0",
    "@reduxjs/toolkit": "^1.3.6",
    "@usedapp/core": "^1.0.5",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.2.9",
    "@web3-react/walletconnect-connector": "^6.2.4",
    "@web3-react/walletlink-connector": "^6.2.13",
    "big.js": "^5.2.2",
    "bignumber.js": "^9.0.2",
    "chart.js": "^3.7.1",
    "chartjs-plugin-annotation": "^1.4.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "decimal.js-light": "^2.5.0",
    "ethers": "^5.6.6",
    "formik": "^2.2.9",
    "jsbi": "^3.1.4",
    "latest": "^0.2.0",
    "lodash": "^4.17.21",
    "next": "latest",
    "next-i18next": "^10.4.0",
    "next-redux-wrapper": "^7.0.5",
    "next-seo": "^5.1.0",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-countup": "^6.1.1",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.3",
    "react-number-format": "^4.9.1",
    "react-redux": "^7.2.0",
    "react-scroll-up": "^1.3.7",
    "react-use": "^17.3.2",
    "react-visibility-sensor": "^5.1.1",
    "remark-breaks": "^3.0.2",
    "sharp": "^0.30.3",
    "swiper": "^8.1.1",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "toformat": "^2.0.0",
    "validator": "^13.7.0",
    "web3-utils": "^1.7.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@emotion/jest": "^11.8.0",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.0.0",
    "@types/big.js": "^6.1.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-redux": "^7.1.18",
    "babel-plugin-transform-jsbi-to-bigint": "^1.4.0",
    "cypress": "^9.5.2",
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.3",
    "jest": "^27.2.0",
    "jest-css-modules-transform": "^4.2.0",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
